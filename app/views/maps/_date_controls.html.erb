<button type="button" class="collapsible" data-target="collapsible.button" data-action="click->collapsible#toggle">Open <%= name %> </button>
<div id="<%= name.tr(" ", "-")  %>" class="filter-panel ml-4" >
    <div class="row mt-3 end">
      <%= form.label("Day", "Day")  %>
      <%= select_tag("day-selector", options_for_select(1..31, "1"))  %>

      <%= form.label("Month", "Month")  %>
      <%= select_tag("month-selector", options_for_select(1..12, "1"))  %>
    </div>
</div>
